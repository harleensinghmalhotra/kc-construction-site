{
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/blog/:slug",
      "destination": "/blog/:slug/index.html"
    },
    {
      "source": "/((?!.+\\.(xml|txt|ico|png|jpg|jpeg|gif|svg|webp|css|js|json|woff|woff2|ttf|eot))(?!blog/).*)",
      "destination": "/index.html"
    }
  ]
}